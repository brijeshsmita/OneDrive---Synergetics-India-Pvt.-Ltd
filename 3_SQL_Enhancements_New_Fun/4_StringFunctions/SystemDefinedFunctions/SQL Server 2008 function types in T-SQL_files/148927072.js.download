            var changeOptions = function(element, object, defaultText) {
                var options = $(element);
                var totalOptions = options.length;
                var i;
                for (i = 0; i < totalOptions; i++) {
                    if (i === 0) {
                        $(options[i]).text(defaultText);
                    }
                    else if (i > 0) {
                        for (x = 0; x < object.length; x++) {
                            var currentTitle = $(options[i]).text();
                            var oldText = object[x].oldText;
                            var newText = object[x].newText;
                            if (currentTitle === oldText){
                                $(options[i]).text(newText);
                            }
                        }
                    }
                }
            };
